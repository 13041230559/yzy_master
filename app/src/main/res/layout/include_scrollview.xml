<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    android:id="@id/ss_content"
    android:background="@color/gray"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
  >

    <LinearLayout
        android:layout_marginBottom="@dimen/dp_20"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <View
            android:layout_width="match_parent"
            android:layout_height="8dp"
            android:background="@color/white0"/>
        <LinearLayout
            android:paddingRight="@dimen/dp_15"
            android:paddingLeft="@dimen/dp_15"
            android:id="@+id/liner_myorder"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@drawable/onclick"
            android:gravity="center"
            android:orientation="horizontal"
          >

            <ImageView
                android:layout_width="@dimen/dp_36"
                android:layout_height="@dimen/dp_36"
                android:scaleType="centerCrop"
                android:src="@mipmap/icon_me_order_yzy"
                />

            <TextView
                android:layout_marginLeft="@dimen/dp_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:text="我的订单"
                android:textColor="@color/black"
                android:textSize="14sp"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_28"
                android:src="@mipmap/icon_back_right_black_yzy"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/fafafa"/>
        <LinearLayout
            android:paddingRight="@dimen/dp_15"
            android:paddingLeft="@dimen/dp_15"
            android:id="@+id/liner_old_myorder"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@drawable/onclick"
            android:gravity="center"
            android:orientation="horizontal"
            >
            <ImageView
                android:layout_width="@dimen/dp_36"
                android:layout_height="@dimen/dp_36"
                android:scaleType="centerCrop"
                android:src="@mipmap/icon_me_history_yzy"
                />
            <TextView
                android:layout_marginLeft="@dimen/dp_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:text="历史订单"
                android:textColor="@color/black"
                android:textSize="14sp"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_28"
                android:src="@mipmap/icon_back_right_black_yzy"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_8"
            android:background="@color/fafafa"/>
        <LinearLayout
            android:visibility="gone"
            android:id="@+id/liner_lucky"
            android:paddingRight="@dimen/dp_15"
            android:paddingLeft="@dimen/dp_15"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@drawable/onclick"
            android:gravity="center"
            android:orientation="horizontal"
            >

            <ImageView
                android:layout_width="@dimen/dp_36"
                android:layout_height="@dimen/dp_36"
                android:scaleType="centerCrop"
                android:src="@mipmap/ic_launcher"
                />

            <TextView
                android:layout_marginLeft="@dimen/dp_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:text="幸运记录"
                android:textColor="@color/black"
                android:textSize="14sp"/>

            <ImageView
                android:layout_width="@dimen/dp_16"
                android:layout_height="@dimen/dp_28"
                android:src="@drawable/right_arrow"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_2"
            android:background="@color/fafafa"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_2"
            android:background="@color/fafafa"/>
        <LinearLayout
            android:id="@+id/liner_my_release"
            android:paddingRight="@dimen/dp_15"
            android:paddingLeft="@dimen/dp_15"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@drawable/onclick"
            android:gravity="center"
            android:orientation="horizontal"
            >

            <ImageView
                android:layout_width="@dimen/dp_36"
                android:layout_height="@dimen/dp_36"
                android:scaleType="centerCrop"
                android:src="@mipmap/icon_me_mypublish_yzy"
                />

            <TextView
                android:layout_marginLeft="@dimen/dp_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:text="我的发布"
                android:textColor="@color/black"
                android:textSize="14sp"/>

            <ImageView
                android:layout_width="@dimen/dp_16"
                android:layout_height="@dimen/dp_28"
                android:src="@mipmap/icon_back_right_black_yzy"
                />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_2"
            android:background="@color/fafafa"/>
        <LinearLayout
            android:id="@+id/liner_my_yudu"
            android:paddingRight="@dimen/dp_15"
            android:paddingLeft="@dimen/dp_15"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@drawable/onclick"
            android:gravity="center"
            android:orientation="horizontal"
            >

            <ImageView
                android:layout_width="@dimen/dp_36"
                android:layout_height="@dimen/dp_36"
                android:scaleType="centerCrop"
                android:src="@mipmap/icon_me_yudou_yzy"
                />

            <TextView
                android:layout_marginLeft="@dimen/dp_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:text="我的娱豆"
                android:textColor="@color/black"
                android:textSize="14sp"/>
            <TextView
                android:id="@+id/my_beannumber"
                android:layout_marginLeft="@dimen/dp_8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="0.0"
                android:layout_marginRight="@dimen/dp_20"
                android:textColor="@color/fea94e"
                android:textSize="14sp"/>

            <ImageView
                android:layout_width="@dimen/dp_16"
                android:layout_height="@dimen/dp_28"
                android:src="@mipmap/icon_back_right_black_yzy"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_2"
            android:background="@color/fafafa"/>
     <LinearLayout
         android:id="@+id/liner_address"
            android:paddingRight="@dimen/dp_15"
            android:paddingLeft="@dimen/dp_15"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@drawable/onclick"
            android:gravity="center"
            android:orientation="horizontal"
            >

            <ImageView
                android:layout_width="@dimen/dp_36"
                android:layout_height="@dimen/dp_36"
                android:scaleType="centerCrop"
                android:src="@mipmap/icon_me_address_yzy"
                />

            <TextView
                android:layout_marginLeft="@dimen/dp_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:text="我的地址"
                android:textColor="@color/black"
                android:textSize="14sp"/>

         <ImageView
             android:layout_width="@dimen/dp_16"
             android:layout_height="@dimen/dp_28"
             android:src="@mipmap/icon_back_right_black_yzy"
             />
        </LinearLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_8"
            android:background="@color/fafafa"/>
        <LinearLayout
            android:id="@+id/liner_my_setup"
            android:paddingRight="@dimen/dp_15"
            android:paddingLeft="@dimen/dp_15"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@drawable/onclick"
            android:gravity="center"
            android:orientation="horizontal"
            >

            <ImageView
                android:layout_width="@dimen/dp_36"
                android:layout_height="@dimen/dp_36"
                android:scaleType="centerCrop"
                android:src="@mipmap/icon_me_sit_yzy"
                />

            <TextView
                android:layout_marginLeft="@dimen/dp_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:text="设      置"
                android:textColor="@color/black"
                android:textSize="14sp"/>

            <ImageView
                android:layout_width="@dimen/dp_16"
                android:layout_height="@dimen/dp_28"
                android:src="@mipmap/icon_back_right_black_yzy"
                />
        </LinearLayout>
        <View
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_30"
            android:background="@color/white"/>

    </LinearLayout>

</android.support.v4.widget.NestedScrollView>
